node (n1){

   stage('SCM') {
      // git clone
git branch: 'sprint1', url: 'https://github.com/Jagannath92/java.master.git'
   }
   
   stage ('build the packages') {
      // mvn package
	  sh 'mvn clean'
   }

   
   
   stage ('archival') {
     // archiving artifacts
	 archiveArtifacts 'target/*.jar'
   }
   stage ('clean') (
	cleanWs() 
}
}
